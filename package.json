{
  "name": "git-branch",
  "version": "0.0.1",
  "packageManager": "pnpm@7.1.1",
  "description": "build bundle of vue2 in esbuild",
  "keywords": [
    "git",
    "git-branch"
  ],
  "bin": {
    "git-branch": "bin/index.mjs"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "bin"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "release": "bumpp && npm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.23.1",
    "@types/node": "^17.0.34",
    "bumpp": "^7.1.1",
    "cypress": "^10.0.3",
    "esbuild": "^0.14.47",
    "eslint": "^8.15.0",
    "nodemon": "^2.0.16",
    "rimraf": "^3.0.2",
    "rollup": "^2.74.0",
    "tsup": "^5.12.8",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vitest": "^0.12.6"
  },
  "homepage": "https://github.com/wulinsheng123/git-branch#readme",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wulinsheng123/git-branch.git"
  },
  "bugs": {
    "url": "https://github.com/wulinsheng123/git-branch/issues"
  },
  "dependencies": {
    "@types/debug": "^4.1.7",
    "@types/hash-sum": "^1.0.0",
    "@types/inquirer": "^8.2.1",
    "chalk": "^5.0.1",
    "debug": "^4.3.4",
    "fsxx": "^0.1.0",
    "hash-sum": "^2.0.0",
    "inquirer": "^9.0.0",
    "ora": "^6.1.2",
    "simple-git": "^3.10.0",
    "zx": "^7.0.7"
  }
}